    <property name="can_focus">False</property>
    <property name="border_width">5</property>
    <property name="title" translatable="yes">About</property>
    <property name="icon">../media/ubuntu-touch-manager.svg</property>
    <property name="type_hint">normal</property>
    <property name="program_name">Ubuntu Touch Manager</property>
    <property name="version">0.1.8.4</property>
    <property name="copyright" translatable="yes">Copyright (C) 2013 Szymon Waliczek majsterrr@gmail.com</property>
    <property name="website">https://launchpad.net/ubuntu-touch-manager</property>
    <property name="license" translatable="yes"># Copyright (C) 2013 Szymon Waliczek majsterrr@gmail.com
